    "@sendgrid/mail": "^7.7.0",
    "@sentry/remix": "^7.32.1",
    "@tailwindcss/forms": "^0.5.6",
    "chart.js": "^4.4.0",
    "date-fns": "^2.29.3",
    "dotenv": "16.0.3",
    "draft-js": "^0.11.7",
    "is-hotkey": "^0.2.0",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.552.0",
    "modern-normalize": "1.1.0",
    "multiselect-react-dropdown": "^2.0.25",
    "next": "^13.0.6",
    "nodemailer": "^6.8.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-autocomplete": "^1.8.1",
    "react-autocomplete-input": "^1.0.20",
    "react-calendar": "^4.6.0",
